set force_conservative 0 
if {$force_conservative} {
	set send_slow {1 .1}
	proc send {ignore arg} {
		sleep .1
		exp_send -s -- $arg
	}
}

set timeout 60

match_max 100000

spawn npx hardhat --config hardhat.config.with-valid-safe-networks.ts lz:validate-safe-configs

expect "All safe configs are valid!"

expect eof
