  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/index.*"
  ],
  "scripts": {
    "prebuild": "npx tsc --noEmit -p tsconfig.build.json",
    "build": "npx tsup",
    "clean": "rm -rf dist",
    "dev": "npx tsup --watch",
    "lint": "npx eslint '**/*.{js,ts,json}'",
    "test": "jest"
  },
