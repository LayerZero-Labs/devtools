{
  "name": "@layerzerolabs/protocol-devtools-evm",
  "version": "3.0.4",
  "description": "Utilities for LayerZero EVM protocol contracts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LayerZero-Labs/devtools.git",
    "directory": "packages/protocol-devtools-evm"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/index.*"
  ],
  "scripts": {
    "prebuild": "$npm_execpath tsc --noEmit",
    "build": "$npm_execpath tsup",
    "clean": "rm -rf dist",
    "dev": "$npm_execpath tsup --watch",
    "lint": "$npm_execpath eslint '**/*.{js,ts,json}'",
    "lint:fix": "eslint --fix '**/*.{js,ts,json}'",
    "test": "$npm_execpath test:jest",
    "test:jest": "jest --ci"
  },
  "dependencies": {
    "p-memoize": "~4.0.4"
  },
  "devDependencies": {
    "@ethersproject/abstract-provider": "^5.7.0",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/constants": "^5.7.0",
    "@ethersproject/contracts": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@layerzerolabs/devtools": "~0.4.4",
    "@layerzerolabs/devtools-evm": "~1.0.3",
    "@layerzerolabs/io-devtools": "~0.1.14",
    "@layerzerolabs/lz-definitions": "^3.0.59",
    "@layerzerolabs/lz-evm-sdk-v2": "^3.0.21",
    "@layerzerolabs/protocol-devtools": "~1.1.3",
    "@layerzerolabs/test-devtools": "~0.4.3",
    "@swc/core": "^1.4.0",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "fast-check": "^3.15.1",
    "jest": "^29.7.0",
    "ts-node": "^10.9.2",
    "tslib": "~2.6.2",
    "tsup": "~8.0.1",
    "typescript": "^5.4.4",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "@ethersproject/abstract-provider": "^5.7.0",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/constants": "^5.7.0",
    "@ethersproject/contracts": "^5.7.0",
    "@ethersproject/providers": "^5.7.0",
    "@layerzerolabs/devtools": "~0.4.4",
    "@layerzerolabs/devtools-evm": "~1.0.3",
    "@layerzerolabs/io-devtools": "~0.1.14",
    "@layerzerolabs/lz-definitions": "^3.0.59",
    "@layerzerolabs/protocol-devtools": "~1.1.3",
    "zod": "^3.22.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
