{
  "name": "@layerzerolabs/devtools-solana",
  "version": "2.0.0",
  "description": "Utilities for LayerZero Solana projects",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LayerZero-Labs/devtools.git",
    "directory": "packages/devtools-solana"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/index.*"
  ],
  "scripts": {
    "prebuild": "$npm_execpath tsc --noEmit",
    "build": "$npm_execpath tsup",
    "clean": "rm -rf dist",
    "dev": "$npm_execpath tsup --watch",
    "lint": "$npm_execpath eslint '**/*.{js,ts,json}'",
    "lint:fix": "eslint --fix '**/*.{js,ts,json}'",
    "test": "$npm_execpath test:jest",
    "test:jest": "jest --ci"
  },
  "dependencies": {
    "@safe-global/api-kit": "^1.3.0",
    "@safe-global/protocol-kit": "^1.3.0",
    "@solana-developers/helpers": "^2.8.1",
    "ethers": "^5.7.2",
    "p-memoize": "~4.0.4"
  },
  "devDependencies": {
    "@layerzerolabs/devtools": "~1.0.0",
    "@layerzerolabs/io-devtools": "~0.2.0",
    "@layerzerolabs/lz-definitions": "^3.0.75",
    "@layerzerolabs/test-devtools": "~0.4.6",
    "@layerzerolabs/test-devtools-solana": "~0.0.5",
    "@solana/web3.js": "~1.98.0",
    "@sqds/multisig": "^2.1.3",
    "@swc/core": "^1.4.0",
    "@swc/jest": "^0.2.36",
    "@types/bn.js": "~5.1.5",
    "@types/jest": "^29.5.12",
    "bn.js": "^5.2.0",
    "fast-check": "^3.15.1",
    "fp-ts": "^2.16.2",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "ts-node": "^10.9.2",
    "tslib": "~2.6.2",
    "tsup": "~8.0.1",
    "typescript": "^5.4.4",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "@layerzerolabs/devtools": "~1.0.0",
    "@layerzerolabs/io-devtools": "~0.2.0",
    "@layerzerolabs/lz-definitions": "^3.0.75",
    "@solana/web3.js": "^1.95.8",
    "bn.js": "^5.2.0",
    "fp-ts": "^2.16.2",
    "zod": "^3.22.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
