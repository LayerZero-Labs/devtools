  "keywords": [
    "LayerZero",
    "OmniChain",
    "EndpointV2",
    "EndpointV2Alt",
    "EVM",
    "OApp",
    "OAppAlt"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LayerZero-Labs/devtools.git",
    "directory": "packages/oapp-alt-evm"
  },
  "license": "MIT",
  "exports": {
    "./package.json": "./package.json",
