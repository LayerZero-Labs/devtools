{
  "name": "build-lz-options",
  "version": "0.0.1",
  "private": true,
  "description": "Build LayerZero options with one command",
  "keywords": [
    "LayerZero",
    "OApp",
    "Options"
  ],
  "repository": {
    "type": "git",
