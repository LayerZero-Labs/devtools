{
  "name": "@layerzerolabs/devtools-cli",
  "version": "0.0.5",
  "description": "CLI for configuring LayerZero OApp contracts",
  "keywords": [
    "LayerZero",
    "OApp",
    "CLI"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LayerZero-Labs/devtools.git",
    "directory": "packages/devtools-cli"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "@layerzerolabs/devtools-cli": "./cli.js"
  },
  "files": [
    "cli.js",
    "dist"
  ],
  "scripts": {
    "prebuild": "tsc -noEmit",
    "build": "$npm_execpath tsup --clean",
    "clean": "rm -rf dist",
    "dev": "$npm_execpath tsup --watch",
    "lint": "$npm_execpath eslint '**/*.{js,ts,json}'",
    "lint:fix": "eslint --fix '**/*.{js,ts,json}'",
    "start": "./cli.js"
  },
  "dependencies": {
    "yoga-layout-prebuilt": "^1.10.0"
  },
  "devDependencies": {
    "@layerzerolabs/devtools": "~0.4.0",
    "@layerzerolabs/io-devtools": "~0.1.12",
    "@layerzerolabs/ua-devtools": "~2.0.0",
    "@types/prompts": "^2.4.9",
    "@types/react": "^17.0.74",
    "commander": "^11.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "ink": "^3.2.0",
    "ink-gradient": "^2.0.0",
    "prompts": "^2.4.2",
    "react": "^17.0.2",
    "ts-node": "^10.9.2",
    "tsup": "~8.0.1",
    "typescript": "^5.4.4",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
