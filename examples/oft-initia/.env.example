EVM_PRIVATE_KEY=

# Initia config
INITIA_REST_URL=https://rest.testnet.initia.xyz
INITIA_RPC_URL=https://rpc.testnet.initia.xyz
INITIA_PRIVATE_KEY=
INITIA_ACCOUNT_ADDRESS=
INITIA_KEY_NAME=

# Movement config
MOVEMENT_INDEXER_URL=https://indexer.testnet.movementnetwork.xyz/v1/graphql
MOVEMENT_FULLNODE_URL=https://aptos.testnet.bardock.movementlabs.xyz/v1
MOVEMENT_ACCOUNT_ADDRESS=
MOVEMENT_PRIVATE_KEY=

# Aptos config
# If you are deploying to Aptos chain, the indexer and fullnode urls are fetched by the Aptos SDK and do not need to be specified.
APTOS_ACCOUNT_ADDRESS=
APTOS_PRIVATE_KEY=