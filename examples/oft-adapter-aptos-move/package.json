    "@jest/globals": "^29.7.0",
    "@layerzerolabs/devtools-evm-hardhat": "^2.0.7",
    "@layerzerolabs/devtools-extensible-cli": "^0.0.3",
    "@layerzerolabs/devtools-move": "^0.0.3",
